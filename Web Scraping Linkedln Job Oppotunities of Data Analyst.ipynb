   "source": [
    "# Creating a new dataframe \n",
    "df2 = pd.DataFrame(list(zip(job_titles,company_names,job_desc)), columns =['job_title', 'company_name','job_description'])\n",
    "# Storing the data to csv file\n",
    "df2.to_csv('France_job_oppotunities.csv', index=False)"
   ]
